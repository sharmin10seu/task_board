<h1>New Task</h1><br>
<%= form_for @task, {:url => tasks_url, :html => {:method => :post}} do |f|  %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br /><br />
    <%= f.label :description %>
    <%= f.text_area :description %> <br /><br />
    <%= f.hidden_field :board_id %>
    <%= f.file_field :doc %>
    <%= f.submit  %>
<% end %>

